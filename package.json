{
  "name": "kaggle-digit-recognizer",
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/JamesMcGuigan/kaggle-digit-recognizer.git"
  },
  "scripts": {
    "clean":            "rm -rvf data ../data-images/",
    "start":            "yarn run download;",
    "download":         "kaggle competitions download -c digit-recognizer -p data; unzip data/digit-recognizer.zip -d data; yarn run csv2png",
    "csv2png":          "node --experimental-modules src/utils/csv2png.js",
    "model-random":     "node --experimental-modules src/random/random.js",
    "copy-gcp-storage": "gsutil -m cp -r data-images gs://kaggle-digit-recognizer/"
  },
  "dependencies": {
    "@google-cloud/vision": "^1.3.0",
    "bluebird": "^3.5.5",
    "count-lines-in-file": "^1.1.0",
    "csv": "^5.1.2",
    "csv-parse": "^4.4.6",
    "fs-jetpack": "^2.2.2",
    "jimp": "^0.8.2",
    "lodash": "^4.17.19",
    "neat-csv": "^5.1.0"
  }
}
